{
  "manifest_version": 2,
  "name": "Grade",
  "version": "0.1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "background",
    "cookies",
    "storage",
    "https://*.gradescope.com/"
  ],
  "browser_action": {
    "default_title": "Show Gradescope summary",
    "default_popup": "index.html"
  },
  "icons":{
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}