{
  "manifest_version": 2,
  "name": "Grade",
  "version": "0.1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "background",
    "cookies",
    "storage",
    "https://*.gradescope.com/"
  ],
  "browser_action": {
    "default_title": "Show Gradescope summary",
    "default_popup": "popup.html"
  }
}